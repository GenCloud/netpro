<?xml version="1.0" encoding="UTF-8"?>
<!-- Client's country ID DWORD added to the end of the packet -->
<packet id="CM_C_LOGIN" xmlns="http://www.l2emu-unique.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.l2emu-unique.net ../../../schemata/packet.xsd">
	<string alias="Account name">
		<scriptAlias id="adp_account_name"/>
	</string>
	<!-- Data known to and verified by the game server -->
	<dword alias="Account ID">
		<scriptAlias id="adp_account_pk"/>
	</dword>
	<dword alias="Session ID" />
	<!-- Data passed back to the login server -->
	<dword alias="Account ID" />
	<dword alias="Auth key" />
	
	<!-- NEW -->
	<dword alias="Localization" type="Language" />
</packet>