<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import org.google.jhsheets.filtered.FilteredTableView?>
<?import org.google.jhsheets.filtered.tablecolumn.FilterableStringTableColumn?>

<SplitPane dividerPositions="0.6" prefHeight="352.0" prefWidth="596.0" xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.l2emuproject.proxy.ui.javafx.packet.view.PacketLogTabController">
   <items>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <bottom>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#clearTable" text="%packettab.footer.clear.table" GridPane.rowIndex="1" />
                  <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#clearMemory" text="%packettab.footer.clear.memory" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <FlowPane alignment="CENTER">
                     <children>
                        <Label fx:id="_labTablePacketCount" text="%packettab.footer.count.table" />
                     </children>
                  </FlowPane>
                  <FlowPane alignment="CENTER" GridPane.columnIndex="1">
                     <children>
                        <Label fx:id="_labMemoryPacketCount" text="%packettab.footer.count.memory" />
                     </children>
                  </FlowPane>
               </children>
            </GridPane>
         </bottom>
         <center>
            <FilteredTableView fx:id="_tvPackets" prefHeight="200.0" prefWidth="200.0" stylesheets="@PacketTable.css" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="_colSender" editable="false" maxWidth="20.0" minWidth="20.0" prefWidth="20.0" text="%packettab.table.column.sender" />
                <FilterableStringTableColumn fx:id="_colOpcode" editable="false" maxWidth="180.0" minWidth="80.0" text="%packettab.table.column.opcodes" />
                <FilterableStringTableColumn fx:id="_colName" editable="false" maxWidth="1.7976931348623157E308" minWidth="120.0" prefWidth="193.0" text="%packettab.table.column.name" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </FilteredTableView>
         </center>
      </BorderPane>
      <fx:include source="PacketInterpView.fxml" />
   </items>
</SplitPane>
