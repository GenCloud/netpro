                              <Insets />
                           </BorderPane.margin>
                        </GridPane>
                     </bottom>
                  </BorderPane>
               </content></Tab>
          <Tab id="connection_pane/idle" text="%main.idletab.title">
               <content>
                  <FlowPane alignment="TOP_CENTER">
                     <children>
                        <Label text="%main.idletab.description" />
                     </children>
                  </FlowPane>
               </content>
          </Tab>
        </tabs>
      </TabPane>
